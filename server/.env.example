# Server Environment Variables

# MongoDB
MONGODB_URI=mongodb://localhost:27017/veribase

# Pinata (IPFS)
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_API_KEY=your_pinata_secret_key

# Blockchain Configuration (Base Sepolia)
BLOCKCHAIN_RPC_URL=https://sepolia.base.org
ADMIN_PRIVATE_KEY=0x...  # Private key of admin wallet (pays gas for minting)

# NFT Contract Addresses (Get from blockchain/deployments/nfts-baseSepolia.json after deployment)
NATIONAL_ID_NFT_ADDRESS=0x...
LAND_OWNERSHIP_NFT_ADDRESS=0x...
LAND_TRANSFER_CONTRACT_ADDRESS=0x...

# Server
PORT=6969
